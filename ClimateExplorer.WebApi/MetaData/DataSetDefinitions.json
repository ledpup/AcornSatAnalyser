        "NullValue": "-9999",
        "ValueAdjustment": 100
      },
      {
        "DataType": "TempMean",
        "UnitOfMeasure": "DegreesCelsius",
        "DataResolution": "Monthly",
        "DataAdjustment": "Unadjusted",
        "RowDataType": "TwelveMonthsPerRow",
        "FolderName": "Temperature\\GHCNm\\Unadjusted",
        "FileNameFormat": "[station].csv",
        "DataRowRegEx": "^(?\u003Cyear\u003E\\d{4}),(-?\\d\u002B),(-?\\d\u002B),(-?\\d\u002B),(-?\\d\u002B),(-?\\d\u002B),(-?\\d\u002B),(-?\\d\u002B),(-?\\d\u002B),(-?\\d\u002B),(-?\\d\u002B),(-?\\d\u002B),(-?\\d\u002B)$",
